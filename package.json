{
  "name": "remote",
  "displayName": "video-remote-vscode",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:remote.playpause",
    "onCommand:remote.rewind_back",
    "onCommand:remote.rewind_forward"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "remote.playpause",
        "title": "Play/Pause your video"
      },
      {
        "command": "remote.rewind_back",
        "title": "Rewind back 10 seconds"
      },
      {
        "command": "remote.rewind_forward",
        "title": "Rewind forward 10 seconds"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  },
  "dependencies": {
    "ws" : "^7.3.1"
  }
}
